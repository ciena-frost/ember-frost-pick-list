{
  "name": "ember-frost-pick-list",
  "version": "1.0.1",
  "description": "The default blueprint for ember-cli addons.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "eslint": "eslint *.js addon app blueprints config tests",
    "lint": "lint-all-the-things",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": "git@github.com:ciena-frost/ember-frost-pick-list.git",
  "engines": {
    "node": ">= 6"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^2.4.5",
    "chai-jquery": "^2.0.0",
    "ember-browserify": "^1.2.0",
    "ember-cli": "2.12.3",
    "ember-cli-chai": "0.4.3",
    "ember-cli-code-coverage": "0.3.12",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-frost-blueprints": "^5.0.1",
    "ember-cli-htmlbars-inline-precompile": "0.3.12",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.14.4",
    "ember-cli-notifications": "^4.2.1",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-uglify": "^1.2.0",
    "ember-code-snippet": "1.7.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-frost-list": "^9.0.0",
    "ember-frost-test": "^4.0.0",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-simple-uuid": "^0.1.4",
    "ember-sinon": "^0.7.0",
    "ember-source": "~2.12.0",
    "ember-test-utils": "^8.1.0",
    "ember-truth-helpers": "^1.2.0",
    "loader.js": "^4.2.3",
    "sinon-chai": "^2.14.0",
    "smoke-and-mirrors": "github:ciena-frost/smoke-and-mirrors"
  },
  "keywords": [
    "ember-addon",
    "frost"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.7",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-sass": "5.6.0",
    "ember-computed-decorators": "~0.3.0",
    "ember-frost-core": "^5.1.1",
    "ember-frost-sort": "^10.0.0",
    "ember-frost-table": "^2.0.0",
    "ember-hook": "1.4.2",
    "ember-prop-types": "^6.0.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "pr-bumper": {
    "coverage": 80
  }
}
